<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="20dp"
    android:paddingTop="20dp"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="160dp"
        android:gravity="center"
        android:orientation="horizontal"
        android:paddingLeft="10dp"
        android:paddingRight="10dp">

        <com.humming.asc.sales.component.DetailsItem
            android:id="@+id/content_customer_details__customer_message"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            app:icons="@drawable/icon_customer1"
            app:labels="@string/customer_message">

        </com.humming.asc.sales.component.DetailsItem>

        <com.humming.asc.sales.component.DetailsItem
            android:id="@+id/content_customer_details__ar"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            app:icons="@drawable/icon_ar"
            app:labels="@string/customer_ar">

        </com.humming.asc.sales.component.DetailsItem>

        <com.humming.asc.sales.component.DetailsItem
            android:id="@+id/content_customer_details__customer_order"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            app:icons="@drawable/icon_order"
            app:labels="@string/customer_order">

        </com.humming.asc.sales.component.DetailsItem>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="160dp"
        android:gravity="center"
        android:orientation="horizontal"
        android:paddingLeft="10dp"
        android:paddingRight="10dp">

        <com.humming.asc.sales.component.DetailsItem
            android:id="@+id/content_customer_details__gc"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            app:icons="@drawable/icon_customer1"
            app:labels="@string/customer_cg">

        </com.humming.asc.sales.component.DetailsItem>

        <com.humming.asc.sales.component.DetailsItem
            android:id="@+id/content_customer_details__contact"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            app:icons="@drawable/icon_contract"
            app:labels="@string/customer_contact">

        </com.humming.asc.sales.component.DetailsItem>

        <com.humming.asc.sales.component.DetailsItem
            android:id="@+id/content_customer_details__task"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            app:icons="@drawable/icon_tasks"
            app:labels="@string/customer_task">

        </com.humming.asc.sales.component.DetailsItem>

        <com.humming.asc.sales.component.DetailsItem
            android:id="@+id/content_customer_details__daily_call"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:visibility="gone"
            app:icons="@drawable/icon_dailycall"
            app:labels="@string/customer_dailycall"></com.humming.asc.sales.component.DetailsItem>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="160dp"
        android:gravity="center"
        android:orientation="horizontal"
        android:paddingLeft="10dp"
        android:paddingRight="10dp">

        <com.humming.asc.sales.component.DetailsItem
            android:id="@+id/content_customer_details__keyaccount_daily_call"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            app:icons="@drawable/icon_dailycall"
            app:labels="@string/customer_dailycall">

        </com.humming.asc.sales.component.DetailsItem>

        <com.humming.asc.sales.component.DetailsItem
            android:id="@+id/content_customer_details__mf_request"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            app:icons="@drawable/icon_mf_request"
            app:labels="@string/customer_mf_request"></com.humming.asc.sales.component.DetailsItem>

        <com.humming.asc.sales.component.DetailsItem
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"></com.humming.asc.sales.component.DetailsItem>

        <com.humming.asc.sales.component.DetailsItem
            android:id="@+id/content_customer_details__null"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:visibility="gone"></com.humming.asc.sales.component.DetailsItem>
    </LinearLayout>
