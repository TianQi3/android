<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/f0f0f0"
    android:orientation="vertical"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context=".activity.settings.PositionListActivity"
    tools:showIn="@layout/activity_position_list">

    <com.humming.asc.sales.component.Loading
        android:id="@+id/activity_loading"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/f0f0f0"
        android:gravity="center" />

    <ListView
        android:id="@+id/content_position__listViews"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:divider="@color/ffbfa574"
        android:dividerHeight="1px"
        android:listSelector="#00000000"
        android:visibility="gone"></ListView>
</LinearLayout>